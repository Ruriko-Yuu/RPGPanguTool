{
  "name": "rpg-pangu-tool",
  "version": "0.0.1",
  "main": "dist-electron/main.js",
  "repository": "git@github.com:Ruriko-Yuu/RPGPanguTool.git",
  "author": "Ruriko <13957619725@163.com>",
  "scripts": {
    "build:e": "webpack --config ./webpack/electron.config.js",
    "build:i": "webpack --config ./webpack/interface.config.js",
    "start:i": "webpack-dev-server --config ./webpack/interface.config.js",
    "start:e": "concurrently \"yarn start:i\" \"yarn build:e & electron .\"",
    "start:e2": "concurrently \"yarn build:e\" \"yarn electron\"",
    "electron": "electron .",
    "start": "yarn start:e",
    "electron:ts": "tsc ./electron/main.ts && electron ./electron/main.js",
    "dist": "electron-builder"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "antd": "^5.8.2",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "electron-is-dev": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "i18next": "^23.2.11",
    "lottie-react": "^2.4.0",
    "mini-css-extract-plugin": "^2.7.6",
    "msgpack-lite": "^0.1.26",
    "pixi-live2d-display": "^0.4.0",
    "pixi.js": "^6.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-router-dom": "^7.1.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "simplex-noise": "^4.0.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "underscore": "^1.13.7",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "zeromq": "^6.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "electron": "^25.3.1",
    "electron-builder": "^24.6.3"
  },
  "build": {
    "appId": "RPT",
    "copyright": "Copyright Â© 2023 ${author}",
    "productName": "rpgPanguTool",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "background": "res/background.png",
      "icon": "build/icons/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 180,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 180,
          "type": "file"
        }
      ],
      "window": {
        "x": 100,
        "y": 100,
        "width": 500,
        "height": 300
      }
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "build/icons/food.png",
      "target": {
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      }
    },
    "files": [
      "dist-interface",
      "dist-electron",
      "build/**/*",
      "public/preload.js"
    ],
    "extends": null,
    "asar": true
  }
}
